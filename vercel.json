{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/wp-content/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/wp-includes/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/about(.*)",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/portfolio(.*)",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/services(.*)",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/contact(.*)",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/index.php/(.*)",
      "destination": "/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
